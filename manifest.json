{
  	"name": "Quotes New Tab",
    "author": "Emil Carlsson",
  	"description": "A new motivational quote in every new tab.",
  	"version": "2.0.0",
  	"manifest_version": 2,
  	"permissions" : [
      "management",
      "topSites"
    ],
  	"chrome_url_overrides" : {
  		"newtab" : "index.html"
  	},
  	"icons": { 
  		"16":  "icons/icon16.png",
      "48":  "icons/icon48.png",
      "128": "icons/icon128.png" 
    },
    "offline_enabled": true
}